  "editor.tokenColorCustomizations": {
    "[Ayu Light]": {
      "textMateRules": [
        {
          "scope": "meta.module",
          "settings": {
            "foreground": "#4cbf99"
          }
        },
        {
          "scope": "constant",
          "settings": {
            "foreground": "#399ee6"
          }
        },
        {
          "scope": "entity.name.record.field.accessor.elm",
          "settings": {
            "foreground": "#6c7680ff"
          }
        }
      ]
    }
  },

